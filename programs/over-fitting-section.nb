        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"dx", "=", 
          RowBox[{
           RowBox[{"data", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "\"\<x\>\""}], "]"}], "]"}], "-", "xm"}]}], 
         ";", "\[IndentingNewLine]", 
         RowBox[{"ssxx", "=", 
          RowBox[{"ssxx", "+", 
           RowBox[{"dx", "*", "dx"}]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"ssxy", "=", 
          RowBox[{"ssxy", "+", 
           RowBox[{"dx", "*", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"data", "[", 
               RowBox[{"[", 
                RowBox[{"i", ",", "\"\<y\>\""}], "]"}], "]"}], "-", "ym"}], 
             ")"}]}]}]}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"beta", "=", 
       RowBox[{"ssxy", "/", "ssxx"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"alpha", "=", 
       RowBox[{"ym", "-", 
        RowBox[{"beta", "*", "xm"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"{", 
        RowBox[{"alpha", ",", "beta"}], "}"}], "]"}]}]}], " ", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{"i", ".", "e", "."}], ",", " ", 
