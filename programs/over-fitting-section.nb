Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"polynomialToString", "[", 
    RowBox[{"vector_", ",", 
     RowBox[{"variable_", ":", "\"\<x\>\""}]}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"power", ",", 
       RowBox[{"n", "=", 
        RowBox[{"Length", "[", "vector", "]"}]}], ",", 
       RowBox[{"string", "=", "\"\<\>\""}], ",", "c"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"power", "=", "0"}], ",", 
        RowBox[{"power", "<", "n"}], ",", 
        RowBox[{"power", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"vector", "[", 
            RowBox[{"[", 
             RowBox[{"power", "+", "1"}], "]"}], "]"}], "\[NotEqual]", "0"}], 
          ",", 
          RowBox[{"(*", " ", 
           RowBox[{
            RowBox[{"NB", " ", "0.0"}], "\[Equal]", 
            RowBox[{"0", " ", "in", " ", "Mathematica"}]}], " ", "*)"}], 
          "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"c", "=", 
            RowBox[{"vector", "[", 
             RowBox[{"[", 
              RowBox[{"power", "+", "1"}], "]"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"(*", " ", 
            RowBox[{
             RowBox[{
             "replace", " ", "Real", " ", "coefficients", " ", "equal", " ", 
              "to", " ", "Integers"}], ",", " ", 
             RowBox[{
             "like", " ", "3.", " ", "to", " ", "exact", " ", "Integer"}]}], 
            " ", "*)"}], "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Round", "[", "c", "]"}], "\[Equal]", "c"}], ",", 
             RowBox[{"c", "=", 
              RowBox[{"Round", "[", "c", "]"}]}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"string", "=", 
            RowBox[{
            "string", "<>", "\"\<+\>\"", "<>", "\[IndentingNewLine]", 
             RowBox[{"ToString", "[", 
              RowBox[{"NumberForm", "[", 
               RowBox[{"c", ",", 
                RowBox[{"{", 
                 RowBox[{"1000", ",", "decimalPlaces"}], "}"}]}], "]"}], 
              "]"}], "<>", "\[IndentingNewLine]", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{"power", "\[Equal]", "0"}], ",", "\"\<\>\"", ",", 
               RowBox[{"variable", "<>", "\[IndentingNewLine]", 
                RowBox[{"If", "[", 
                 RowBox[{
                  RowBox[{"power", "\[Equal]", "1"}], ",", "\"\<\>\"", ",", 
                  RowBox[{"\"\<^\>\"", "<>", 
                   RowBox[{"ToString", "[", "power", "]"}]}]}], "]"}]}]}], 
              "\[IndentingNewLine]", "]"}]}]}]}]}], "\[IndentingNewLine]", 
         "]"}]}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Fold", "[", 
       RowBox[{"StringReplace", ",", "string", ",", 
        RowBox[{"{", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"\"\<+1x\>\"", "\[Rule]", "\"\<+x\>\""}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"StartOfString", "~~", "\"\<+\>\""}], "\[Rule]", 
           "\"\<\>\""}], ",", "\[IndentingNewLine]", 
          RowBox[{"\"\<-1x\>\"", "\[Rule]", "\"\<-x\>\""}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"\"\<-1x\>\"", "\[Rule]", "\"\<-x\>\""}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"\"\<+-\>\"", "\[Rule]", "\"\<-\>\""}]}], 
         "\[IndentingNewLine]", "}"}]}], "\[IndentingNewLine]", "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"polynomialToString", "[", 
