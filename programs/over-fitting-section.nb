Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"LagrangePolynomial", "[", "data_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"n", "=", 
        RowBox[{"Length", "[", "data", "]"}]}], ",", "sum", ",", "i", ",", 
       "j", ",", "m", ",", "product", ",", "divisor"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"sum", "=", 
       RowBox[{"Table", "[", 
        RowBox[{"0", ",", "n"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"j", "=", "1"}], ",", 
        RowBox[{"j", "\[LessEqual]", "n"}], ",", 
        RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"product", "=", 
          RowBox[{"Table", "[", 
           RowBox[{"0", ",", "n"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"product", "[", 
           RowBox[{"[", "1", "]"}], "]"}], "=", 
          RowBox[{"data", "[", 
           RowBox[{"[", 
            RowBox[{"j", ",", "\"\<y\>\""}], "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"For", "[", " ", 
          RowBox[{
           RowBox[{"m", "=", "1"}], ",", 
           RowBox[{"m", "\[LessEqual]", "n"}], ",", 
           RowBox[{"m", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"m", "\[NotEqual]", "j"}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"divisor", "=", 
               RowBox[{
                RowBox[{"data", "[", 
                 RowBox[{"[", 
                  RowBox[{"j", ",", "\"\<x\>\""}], "]"}], "]"}], "-", 
                RowBox[{"data", "[", 
                 RowBox[{"[", 
                  RowBox[{"m", ",", "\"\<x\>\""}], "]"}], "]"}]}]}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"For", "[", " ", 
               RowBox[{
                RowBox[{"i", "=", "n"}], ",", 
                RowBox[{"i", "\[GreaterEqual]", "1"}], ",", 
                RowBox[{"i", "--"}], ",", 
                RowBox[{"(*", " ", 
                 RowBox[{"multiply", " ", "product", " ", "by", " ", 
                  RowBox[{
                   RowBox[{"(", 
                    RowBox[{"x", "-", 
                    RowBox[{"data", "[", 
                    RowBox[{"[", 
                    RowBox[{"m", ",", "\"\<x\>\""}], "]"}], "]"}]}], ")"}], 
                   "/", "divisor"}]}], " ", "*)"}], "\[IndentingNewLine]", 
                " ", 
                RowBox[{
                 RowBox[{
                  RowBox[{"product", "[", 
                   RowBox[{"[", "i", "]"}], "]"}], "=", 
                  RowBox[{
                   RowBox[{"(", 
                    RowBox[{
                    RowBox[{"If", "[", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"i", "-", "1"}], "\[GreaterEqual]", "1"}], ",", 
                    RowBox[{"product", "[", 
                    RowBox[{"[", 
                    RowBox[{"i", "-", "1"}], "]"}], "]"}], ",", "0"}], "]"}], 
                    "-", 
                    RowBox[{
                    RowBox[{"product", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], "*", 
                    RowBox[{"data", "[", 
                    RowBox[{"[", 
                    RowBox[{"m", ",", "\"\<x\>\""}], "]"}], "]"}]}]}], ")"}], 
                   "/", "divisor"}]}], ";"}]}], "\[IndentingNewLine]", "]"}], 
              ";"}]}], "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
          "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"For", "[", " ", 
          RowBox[{
           RowBox[{"i", "=", "1"}], ",", 
           RowBox[{"i", "\[LessEqual]", "n"}], ",", 
           RowBox[{"i", "++"}], ",", 
           RowBox[{"(*", " ", 
            RowBox[{
            "add", " ", "the", " ", "product", " ", "coefficients", " ", "to",
              " ", "running", " ", "sum"}], " ", "*)"}], 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{
             RowBox[{"sum", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "=", 
             RowBox[{
              RowBox[{"sum", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "+", 
              RowBox[{"product", "[", 
               RowBox[{"[", "i", "]"}], "]"}]}]}], ";"}]}], 
          "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", "]"}], ";",
       " ", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "sum", "]"}]}]}], "\n", "]"}]}], ";"}]], "Input",\

