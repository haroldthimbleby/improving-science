Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Leastsquares", "[", "data_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"n", "=", 
        RowBox[{"Length", "[", "data", "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"ssxx", "=", "0"}], ",", " ", 
       RowBox[{"(*", " ", 
        RowBox[{"sum", " ", "squares", " ", 
         RowBox[{"(", 
          RowBox[{"x", "-", "xmean"}], ")"}]}], "*)"}], " ", 
       "\[IndentingNewLine]", 
       RowBox[{"ssxy", "=", "0"}], ",", " ", 
       RowBox[{"(*", " ", 
        RowBox[{"sum", " ", "of", " ", 
         RowBox[{"(", 
          RowBox[{"x", "-", "xmean"}], ")"}], "*", 
         RowBox[{"(", 
          RowBox[{"y", "-", "ymean"}], ")"}]}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"xm", "=", "0"}], ",", " ", 
       RowBox[{"(*", " ", 
        RowBox[{"x", " ", "mean"}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"ym", "=", "0"}], ",", " ", 
       RowBox[{"(*", " ", "mean", " ", "*)"}], "\[IndentingNewLine]", "alpha",
        ",", "beta", ",", "i", ",", "dx"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", 
        RowBox[{"i", "\[LessEqual]", "n"}], ",", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"xm", "=", 
          RowBox[{"xm", "+", 
           RowBox[{"data", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "\"\<x\>\""}], "]"}], "]"}]}]}], ";", 
